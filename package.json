{
  "name": "hubot-geo",
  "version": "0.1.0",
  "description": "Hubot script to provide Geo info about an IP or DNS name ",
  "main": "index.js",
  "scripts": {
    "test": "node ./node_modules/mocha/bin/mocha --compilers coffee:coffee-script/register --recursive -R list",
    "test-watch": "node ./node_modules/mocha/bin/mocha --compilers coffee:coffee-script/register --recursive -R list -w"
  },
  "repository": {
    "type": "git",
