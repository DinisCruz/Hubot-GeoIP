  "license": "MIT",
  "bugs": {
    "url": "https://github.com/DinisCruz/hubot-geo/issues"
  },
  "homepage": "https://github.com/DinisCruz/hubot-geo",
  "devDependencies": {
    "mocha": "^2.2.5"
  },
  "dependencies": {
    "coffee-script": "^1.9.2",
    "fluentnode": "^0.4.0",
    "hubot": "^2.12.0"
  }
}
